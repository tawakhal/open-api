        }
      }
    },
    "protoLoginRequest": {
      "type": "object",
      "properties": {
        "username": {
          "type": "string",
          "title": "value of username when want login"
        },
        "password": {
          "type": "string",
          "title": "value of password when want login"
        }
      }
    }
  },
  "securityDefinitions": {
    "bearer": {
      "type": "apiKey",
      "description": "Authentication token, prefixed by Bearer: Bearer \u003ctoken\u003e",
